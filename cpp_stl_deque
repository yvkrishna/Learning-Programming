#include <bits/stdc++.h>
using namespace std;

int main() {
  cout<<"======= MAIN USEFUL FUNCTIONS ======="<<endl;
	deque<int> d;
	d.push_back(1);
	d.push_front(2);
	d.push_back(3);
	d.push_front(4);
	for(int i=0;i<d.size();i++){
	    cout<<d[i]<<" "<<d.at(i)<<endl;
	}
	d.pop_back();
	d.pop_front();
	for(int i=0;i<d.size();i++){
	    cout<<d[i]<<" ";
	}
	cout<<endl;
	cout<<d.size()<<endl;
	d.erase(d.begin(),d.begin()+1);
	cout<<d.size()<<endl;
	d.push_back(1);
	d.push_front(2);
	d.push_back(3);
	d.push_front(4);
	cout<<d.size()<<endl;
	d.clear();
	cout<<d.size()<<endl;
    cout<<"======";
	return 0;
}
